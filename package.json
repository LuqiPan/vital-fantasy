  "type": "module",
  "scripts": {
    "lint": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "build": "nx run-many -t build",
    "check": "nx run-many -t lint test build",
    "ci": "yarn nx-cloud record -- yarn check",
    "ci:ft-bill": "yarn nx-cloud record -- yarn ft-bill",
    "ci:bielsko-biala": "yarn nx-cloud record -- yarn bielsko-biala",
    "ci:leogang": "yarn nx-cloud record -- yarn leogang",
    "clean": "rm -rf dist; rm -rf coverage; rm -rf tmp",
    "ft-bill": "nx exec -- node dist/apps/ft-bill",
    "bielsko-biala": "nx exec -- node dist/apps/bielsko-biala",
    "leogang": "nx exec -- node dist/apps/leogang"
  },
  "nx": {},
  "devDependencies": {
    "@nx/esbuild": "19.1.2",
    "@nx/eslint": "19.1.2",
    "@nx/eslint-plugin": "19.1.2",
    "@nx/js": "19.1.2",
    "@nx/node": "19.1.2",
    "@nx/vite": "19.1.2",
    "@nx/workspace": "19.1.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "20.13.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "@vitest/coverage-istanbul": "1.6.0",
