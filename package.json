{
  "name": "@vital-fantasy/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "build": "nx run-many -t build",
    "check": "nx run-many -t lint test build --parallel 10",
    "ci": "yarn nx-cloud record -- yarn check",
    "ci:ft-bill": "yarn nx-cloud record -- yarn ft-bill",
    "ci:bielsko-biala": "yarn nx-cloud record -- yarn bielsko-biala",
    "clean": "rm -rf dist; rm -rf coverage; rm -rf tmp",
    "ft-bill": "nx exec -- node dist/apps/ft-bill",
    "bielsko-biala": "nx exec -- node dist/apps/bielsko-biala"
  },
  "nx": {},
  "devDependencies": {
    "@nx/esbuild": "19.1.1",
    "@nx/eslint": "19.1.1",
    "@nx/eslint-plugin": "19.1.1",
    "@nx/js": "19.1.1",
    "@nx/node": "19.1.1",
    "@nx/vite": "19.1.1",
    "@nx/workspace": "19.1.1",
    "@swc-node/register": "~1.8.0",
    "@swc/core": "1.4.17",
    "@swc/helpers": "~0.5.2",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/ui": "^1.3.1",
    "esbuild": "^0.19.2",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "nx": "19.1.1",
    "prettier": "3.2.5",
    "ts-node": "10.9.1",
    "typescript": "~5.4.2",
    "vite": "^5.2.11",
    "vitest": "^1.3.1"
  },
  "packageManager": "yarn@4.2.2",
  "dependencies": {
    "csv-parse": "5.5.6",
    "tslib": "2.6.2"
  }
}
