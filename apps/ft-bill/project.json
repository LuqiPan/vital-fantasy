{
  "name": "ft-bill",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ft-bill/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "platform": "node",
        "outputPath": "dist/apps/ft-bill",
        "format": ["esm"],
        "bundle": true,
        "main": "apps/ft-bill/src/main.ts",
        "tsConfig": "apps/ft-bill/tsconfig.app.json",
        "assets": ["apps/ft-bill/src/assets"],
        "generatePackageJson": true,
        "esbuildOptions": {
          "sourcemap": false,
          "outExtension": {
            ".js": ".js"
          }
        }
      }
    }
  }
}
