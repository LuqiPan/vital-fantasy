{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "esnext",
    "lib": ["esnext"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "paths": {
      "@vital-fantasy/analyze-round": ["libs/analyze-round/src/index.ts"],
      "@vital-fantasy/combine": ["libs/combine/src/index.ts"],
      "@vital-fantasy/constants": ["libs/constants/src/index.ts"],
      "@vital-fantasy/parse-csv": ["libs/parse-csv/src/index.ts"],
      "@vital-fantasy/parse-results": ["libs/parse-results/src/index.ts"],
      "@vital-fantasy/parse-stats": ["libs/parse-stats/src/index.ts"],
      "@vital-fantasy/string-for-print": ["libs/string-for-print/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
