{
  "name": "parse",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/parse/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/parse",
        "main": "packages/parse/src/index.ts",
        "tsConfig": "packages/parse/tsconfig.lib.json",
        "assets": ["packages/parse/*.md"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/packages/parse"
      }
    }
  }
}
